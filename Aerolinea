CREATE TABLE AEROLINEA (ID_AEROLINEA INTEGER, NOMBRE VARCHAR(90),
CONSTRAINT ID_AEROLINEA_PK PRIMARY KEY (ID_AEROLINEA));
--Las secuencias van asociadas a primary keys que se incrementan solitas
--CREATE OR REPLACE //
-- Puede modificarse el codigo creado
--Entre el AS y el begin se declaran las variables que se van a usar 








CREATE OR REPLACE PROCEDURE GUARDAR_AVION(
MY_ID_AVION OUT INTEGER, MY_ID_AEROLINEA IN INTEGER,
MY_ASIENTOS IN INTEGER)
AS
BEGIN
SELECT SEC_AVION.NEXTVAL INTO MY_ID_AVION FROM DUAL;
INSERT INTO AVION VALUES (MY_ID_AVION, MY_ID_AEROLINEA,
MY_ASIENTOS);
END;
/
